#:kivy 2.1.0

<LicenseScreen>:
    BoxLayout:
        orientation: 'vertical'
        BoxLayout:
            orientation: 'vertical'
            size_hint_y: 0.9
            ScrollView:
                size_hint_y: 200
                BoxLayout:
                    orientation: 'vertical'
                    size_hint_y: None
                    height: self.minimum_height
                    Label:
                        text: "フォントデータ"
                        font_size: 18
                        size_hint_y: None
                    Label:
                        id: Font
                        font_size: 12
                        size_hint: (1, None)
                        height: self.texture_size[1]
                        halign: 'left'
                    Label:
                        text: ""
                        font_size: 18
                        size_hint_y: None
                    Label:
                        text: "kivy"
                        font_size: 18
                        size_hint_y: None
                    Label:
                        id: kivylicense
                        font_size: 12
                        size_hint: (1, None)
                        height: self.texture_size[1]
                        halign: 'left'
                    Label:
                        text: ""
                        font_size: 18
                        size_hint_y: None
                    Label:
                        text: "requests"
                        font_size: 18
                        size_hint_y: None
                    Label:
                        id: requests
                        font_size: 12
                        size_hint: (1, None)
                        height: self.texture_size[1]
                        halign: 'left'
                    Label:
                        text: ""
                        font_size: 18
                        size_hint_y: None
                    Label:
                        text: "Selenim"
                        font_size: 18
                        size_hint_y: None
                    Label:
                        id: Selenim_Notice
                        font_size: 12
                        size_hint: (1, None)
                        height: self.texture_size[1]
                        halign: 'left'
                    Label:
                        id: Selenim
                        font_size: 12
                        size_hint: (1, None)
                        height: self.texture_size[1]
                        halign: 'left'

        BoxLayout:
            size_hint_y: 0.1
            Button:
                text: '戻る'
                font_size: 18
                on_press:
                    root.manager.current = 'menu'
